<h1>Listing roms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
      <th>Company</th>
      <th>Passed</th>
      <th>Playing</th>
      <th>Savefile</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roms.each do |rom| %>
      <tr>
        <td><%= rom.name %></td>
        <td><%= rom.year %></td>
        <td><%= rom.company %></td>
        <td><%= rom.passed %></td>
        <td><%= rom.playing %></td>
        <td><%= rom.savefile %></td>
        <td><%= rom.comment %></td>
        <td><%= link_to 'Show', rom %></td>
        <td><%= link_to 'Edit', edit_rom_path(rom) %></td>
        <td><%= link_to 'Destroy', rom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rom', new_rom_path %>
